{
    "name": "rundizstrap-companion",
    "version": "0.0.2",
    "description": "The WordPress Blocks for Bootstrap Basic FSE theme based on latest Bootstrap.",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "wpdev build",
        "buildBlocks": "wp-scripts build --webpack-src-dir=\"./blocks-src\" --output-path=\"./blocks\"",
        "langPot": "wp i18n make-pot . --exclude=\"assets/vendor,App/vendor,blocks-src,languages,node_modules,node_tasks,*.css,*.svg,*.eot,*.woff,*.woff2,*.ttf,*.txt,*.pot\"",
        "langJson": "wp i18n make-json languages/rundizstrap-companion-th.po --no-purge --pretty-print",
        "watch": "wpdev watch",
        "watchBlocks": "wp-scripts start --webpack-src-dir=\"./blocks-src\" --output-path=\"./blocks\"",
        "pack": "wpdev pack --packtype=dev prod",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "",
        "url": ""
    },
    "keywords": [
        "bootstrap",
        "wordpress",
        "plugin"
    ],
    "author": "Vee W.",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Rundiz-WP/rundizstrap-companion/issues"
    },
    "homepage": "",
    "dependencies": {
        "ace-builds": "^1.43.6",
        "bootstrap": "^5.3.8",
        "bootstrap-icons": "^1.13.1"
    },
    "devDependencies": {
        "@wordpress/scripts": "^31.3.0"
    }
}