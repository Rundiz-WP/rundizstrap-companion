{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "attributes": {
        "type": {
            "type": "string",
            "default": "next"
        },
        "label": {
            "type": "string",
            "role": "content",
            "default": ""
        },
        "showTitle": {
            "type": "boolean",
            "default": false
        },
        "taxonomy": {
            "type": "string",
            "default": ""
        },
        "prependTextHtml": {
            "type": "string",
            "default": ""
        },
        "appendTextHtml": {
            "type": "string",
            "default": ""
        },
        "rel": {
            "type": "string",
            "default": ""
        },
        "tabindex": {
            "type": "string",
            "default": ""
        },
        "dataAttributes": {
            "type": "object",
            "default": {}
        },
        "ariaAttributes": {
            "type": "object",
            "default": {}
        }
    },
    "category": "theme",
    "description": "Displays the next or previous post link in Bootstrap style. This block is in the BBFSE plugin.",
    "editorScript": "file:./index.js",
    "example": {
        "attributes": {
            "type": "next",
            "label": "Bootstrap Next Post",
            "className": "btn btn-secondary"
        },
        "viewportWidth": 350
    },
    "icon": "arrow-right-alt2",
    "keywords": ["bootstrap", "post", "navigation", "next", "previous"],
    "name": "bbfse-plug/blocks-bs-post-navigation-link",
    "render": "file:./render.php",
    "supports": {
        "html": false,
        "className": true
    },
    "textdomain": "bbfse-plug",
    "title": "Bootstrap post navigation link",
    "usesContext": ["postType"],
    "version": "0.0.1"
}
